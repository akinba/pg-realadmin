<!DOCTYPE html>
<html>
<head>
	<title></title>
	<script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>
	<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.0/js/bootstrap.min.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/lodash@4.17.5/lodash.min.js"></script>
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.0/css/bootstrap.min.css"></link>
</head>
<body>
<div id="app" class="container">
	<div class="row">
		<div class="col-4">
	<ul>
		<li v-for="schema in schemas ">
			{{schema}}
			<ul>
				<li v-for="table in _.filter(tables, {table_schema: schema})" class="alert alert-primary">
					{{table.table_name}}
				</li>
			</ul>
		</li>
	</ul>			
		</div>
		<div class="col-8 alert alert-secondary">2</div>
	</div>

	<ul>
		<li v-for="table in tables">
			{{table}}
		</li>
	</ul>

</div>

<script type="text/javascript">

Vue.component()

var app = new Vue({
  el: '#app',
  data: {
    tables: <%- JSON.stringify(data) %>
  },
  computed: {
  	schemas() {
  		return _.uniqBy(this.tables.map(p=>p.table_schema));
  	}
  }
})

</script>
</body>
</html>






